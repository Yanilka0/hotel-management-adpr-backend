{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "maven: clean install",
            "type": "shell",
            "command": "mvn clean install",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "maven: run",
            "type": "shell",
            "command": "mvn spring-boot:run",
            "group": {
                "kind": "test",
                "isDefault": false
            }
        }
    ]
}